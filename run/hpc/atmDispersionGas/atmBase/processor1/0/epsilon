FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.00273254;

boundaryField
{
    frontField
    {
        type            atmBoundaryLayerInletEpsilon;
        flowDir         ( 1 0 0 );
        zDir            ( 0 0 1 );
        Uref            10;
        Zref            6;
        z0              uniform 0.01;
        zGround         uniform 0;
        value           uniform 0.00273254;
    }
    backField
    {
        type            zeroGradient;
    }
    symPlane
    {
        type            symmetryPlane;
    }
    sideField
    {
        type            atmBoundaryLayerInletEpsilon;
        flowDir         ( 1 0 0 );
        zDir            ( 0 0 1 );
        Uref            10;
        Zref            6;
        z0              uniform 0.01;
        zGround         uniform 0;
        value           uniform 0.00273254;
    }
    ground
    {
        type            epsilonWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0.00273254;
    }
    top
    {
        type            atmBoundaryLayerInletEpsilon;
        flowDir         ( 1 0 0 );
        zDir            ( 0 0 1 );
        Uref            10;
        Zref            6;
        z0              uniform 0.01;
        zGround         uniform 0;
        value           uniform 0.00273254;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0.00273254;
    }
}


