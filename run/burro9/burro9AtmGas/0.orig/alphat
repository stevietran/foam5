FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    vapourInlet
    {
        type            calculated;
        value           uniform 0;
    }
    frontField
    {
        type            calculated;
        value           $internalField;
    }
    backField
    {
        type            calculated;
        value           $internalField;
    }
    sideField
    {
        type            calculated;
        value           $internalField;
    }
    top
    {
        type            calculated;
        value           $internalField;
    }
    ground
    {
        type            compressible::alphatWallFunction;
        Prt             0.85;
        value           uniform 0;
    }
    symPlane
    {
        type            symmetryPlane;
    }
    
}
// ************************************************************************* //
